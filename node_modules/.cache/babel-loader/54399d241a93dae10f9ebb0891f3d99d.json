{"ast":null,"code":"import AppHeader from './components/AppHeader.vue';\nimport AppList from './components/AppList.vue';\nimport AddList from './components/AddList.vue';\nexport default {\n  name: 'App',\n  components: {\n    AppHeader,\n    AppList,\n    AddList\n  },\n\n  data() {\n    return {\n      List: [],\n      showAddList: false\n    };\n  },\n\n  methods: {\n    toggleAddList() {\n      this.showAddList = !this.showAddList;\n    },\n\n    addList(List) {\n      this.List = [...this.List, List];\n    },\n\n    deleteList(id) {\n      if (confirm('Are you sure?')) {\n        this.List = this.List.filter(List => List.id !== id);\n      }\n    },\n\n    toggleReminder(id) {\n      this.List = this.List.map(List => List.id === id ? { ...List,\n        reminder: !List.reminder\n      } : List);\n    }\n\n  },\n\n  created() {\n    this.List = [{\n      id: 1,\n      text: 'List 1',\n      day: 'Some Description',\n      reminder: true\n    }, {\n      id: 2,\n      text: 'List 2',\n      day: 'Some Description',\n      reminder: true\n    }, {\n      id: 3,\n      text: 'List 3',\n      day: 'Some Description',\n      reminder: false\n    }];\n  }\n\n};","map":{"version":3,"mappings":"AAWA,OAAOA,SAAP,MAAsB,4BAAtB;AACA,OAAOC,OAAP,MAAoB,0BAApB;AACA,OAAOC,OAAP,MAAoB,0BAApB;AAEA,eAAe;EACbC,IAAI,EAAE,KADO;EAEbC,UAAU,EAAE;IACVJ,SADU;IAEVC,OAFU;IAGVC;EAHU,CAFC;;EAQbG,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE,EADD;MAELC,WAAW,EAAE;IAFR,CAAP;EAID,CAbY;;EAebC,OAAO,EAAE;IACPC,aAAa,GAAG;MACd,KAAKF,WAAL,GAAmB,CAAC,KAAKA,WAAzB;IACD,CAHM;;IAKPG,OAAO,CAACJ,IAAD,EAAO;MACZ,KAAKA,IAAL,GAAY,CAAC,GAAG,KAAKA,IAAT,EAAeA,IAAf,CAAZ;IACD,CAPM;;IASPK,UAAU,CAACC,EAAD,EAAK;MACb,IAAIC,OAAO,CAAC,eAAD,CAAX,EAA8B;QAC5B,KAAKP,IAAL,GAAY,KAAKA,IAAL,CAAUQ,MAAV,CAAkBR,IAAD,IAAUA,IAAI,CAACM,EAAL,KAAYA,EAAvC,CAAZ;MACF;IACD,CAbM;;IAePG,cAAc,CAACH,EAAD,EAAK;MACjB,KAAKN,IAAL,GAAY,KAAKA,IAAL,CAAUU,GAAV,CAAeV,IAAD,IAAUA,IAAI,CAACM,EAAL,KAAYA,EAAZ,GAAiB,EAAE,GAAGN,IAAL;QAAWW,QAAQ,EAAE,CAACX,IAAI,CAACW;MAA3B,CAAjB