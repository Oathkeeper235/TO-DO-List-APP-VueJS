{"ast":null,"code":"import AppHeader from './components/AppHeader.vue';\nimport AppTasks from './components/AppTasks.vue';\nexport default {\n  name: 'App',\n  components: {\n    AppHeader,\n    AppTasks\n  },\n\n  data() {\n    return {\n      tasks: []\n    };\n  },\n\n  methods: {\n    deleteTask(id) {\n      if (confirm('Are you sure?')) {\n        this.tasks = this.tasks.filter(task => task.id !== id);\n      }\n    },\n\n    toggleReminder(id) {\n      this.tasks = this.tasks.map(task => task.id === id ? { ...task,\n        reminder: !task.reminder\n      } : task);\n    }\n\n  },\n\n  created() {\n    this.tasks = [{\n      id: 1,\n      text: 'Doctors Appointment',\n      day: 'March 1st at 2:30pm',\n      reminder: true\n    }, {\n      id: 2,\n      text: 'Meeting at School',\n      day: 'March 3rd at 1:30pm',\n      reminder: true\n    }, {\n      id: 3,\n      text: 'Food Shopping',\n      day: 'March 3rd at 11:00',\n      reminder: false\n    }];\n  }\n\n};","map":{"version":3,"mappings":"AAQA,OAAOA,SAAP,MAAsB,4BAAtB;AACA,OAAOC,QAAP,MAAqB,2BAArB;AAEA,eAAe;EACbC,IAAI,EAAE,KADO;EAEbC,UAAU,EAAE;IACVH,SADU;IAEVC;EAFU,CAFC;;EAObG,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE;IADF,CAAP;EAGD,CAXY;;EAabC,OAAO,EAAE;IACPC,UAAU,CAACC,EAAD,EAAK;MACb,IAAGC,OAAO,CAAC,eAAD,CAAV,EAA6B;QAC3B,KAAKJ,KAAL,GAAa,KAAKA,KAAL,CAAWK,MAAX,CAAmBC,IAAD,IAAUA,IAAI,CAACH,EAAL,KAAYA,EAAxC,CAAb;MACF;IACD,CALM;;IAOPI,cAAc,CAACJ,EAAD,EAAK;MACjB,KAAKH,KAAL,GAAa,KAAKA,KAAL,CAAWQ,GAAX,CAAgBF,IAAD,IAAUA,IAAI,CAACH,EAAL,KAAYA,EAAZ,GAAiB,EAAC,GAAGG,IAAJ;QAAUG,QAAQ,EAAE,CAACH,IAAI,CAACG;MAA1B,CAAjB,GAAuDH,IAAhF,CAAb;IACF;;EATO,CAbI;;EAyBbI,OAAO,GAAG;IACR,KAAKV,KAAL,GAAa,CACX;MACEG,EAAE,EAAE,CADN;MAEEQ,IAAI,EAAE,qBAFR;MAGEC,GAAG,EAAE,qBAHP;MAIEH,QAAQ,EAAE;IAJZ,CADW,EAOX;MACEN,EAAE,EAAE,CADN;MAEEQ,IAAI,EAAE,mBAFR;MAGEC,GAAG,EAAE,qBAHP;MAIEH,QAAQ,EAAE;IAJZ,CAPW,EAaX;MACEN,EAAE,EAAE,CADN;MAEEQ,IAAI,EAAE,eAFR;MAGEC,GAAG,EAAE,oBAHP;MAIEH,QAAQ,EAAE;IAJZ,CAbW,CAAb;EAoBF;;AA9Ca,CAAf","names":["AppHeader","AppTask