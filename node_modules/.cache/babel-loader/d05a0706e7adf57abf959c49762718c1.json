{"ast":null,"code":"import AppHeader from './components/AppHeader.vue';\nimport AppTasks from './components/AppTasks.vue';\nimport AddTask from './components/AddTask.vue';\nexport default {\n  name: 'App',\n  components: {\n    AppHeader,\n    AppTasks,\n    AddTask\n  },\n\n  data() {\n    return {\n      tasks: []\n    };\n  },\n\n  methods: {\n    addTask(task) {\n      this.tasks = [...this.tasks, task];\n    },\n\n    deleteTask(id) {\n      if (confirm('Are you sure?')) {\n        this.tasks = this.tasks.filter(task => task.id !== id);\n      }\n    },\n\n    toggleReminder(id) {\n      this.tasks = this.tasks.map(task => task.id === id ? { ...task,\n        reminder: !task.reminder\n      } : task);\n    }\n\n  },\n\n  created() {\n    this.tasks = [{\n      id: 1,\n      text: 'List 1',\n      day: 'Some Description',\n      reminder: true\n    }, {\n      id: 2,\n      text: 'List 2',\n      day: 'Some Description',\n      reminder: true\n    }, {\n      id: 3,\n      text: 'List 3',\n      day: 'Some Description',\n      reminder: false\n    }];\n  }\n\n};","map":{"version":3,"mappings":"AASA,OAAOA,SAAP,MAAsB,4BAAtB;AACA,OAAOC,QAAP,MAAqB,2BAArB;AACA,OAAOC,OAAP,MAAoB,0BAApB;AAEA,eAAe;EACbC,IAAI,EAAE,KADO;EAEbC,UAAU,EAAE;IACVJ,SADU;IAEVC,QAFU;IAGVC;EAHU,CAFC;;EAQbG,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE;IADF,CAAP;EAGD,CAZY;;EAcbC,OAAO,EAAE;IACPC,OAAO,CAACC,IAAD,EAAO;MACZ,KAAKH,KAAL,GAAa,CAAC,GAAG,KAAKA,KAAT,EAAgBG,IAAhB,CAAb;IACD,CAHM;;IAIPC,UAAU,CAACC,EAAD,EAAK;MACb,IAAGC,OAAO,CAAC,eAAD,CAAV,EAA6B;QAC3B,KAAKN,KAAL,GAAa,KAAKA,KAAL,CAAWO,MAAX,CAAmBJ,IAAD,IAAUA,IAAI,CAACE,EAAL,KAAYA,EAAxC,CAAb;MACF;IACD,CARM;;IAUPG,cAAc,CAACH,EAAD,EAAK;MACjB,KAAKL,KAAL,GAAa,KAAKA,KAAL,CAAWS,GAAX,CAAgBN,IAAD,IAAUA,IAAI,CAACE,EAAL,KAAYA,EAAZ,GAAiB,EAAC,GAAGF,IAAJ;QAAUO,QAAQ,EAAE,CAACP,IAAI,CAACO;MAA1B,CAAjB,GAAuDP,IAAhF,CAAb;IACF;;EAZO,CAdI;;EA6BbQ,OAAO,GAAG;IACR,KAAKX,KAAL,GAAa,CACX;MACEK,EAAE,EAAE,CADN;MAEEO,IAAI,EAAE,QAFR;MAGEC,GAAG,EAAE,kBAHP;MAIEH,QAAQ,EAAE;IAJZ,CADW,EAOX;MACEL,EAAE,EAAE,CADN;MAEEO,IAAI,EAAE,QAFR;MAGEC,GAAG,EAAE,kBAHP;MAIEH,QAAQ,EAAE;IAJZ,CAPW,EAaX;MACEL,EAAE,EAAE,CADN;MAEEO,IAAI,EAAE,QAFR;MAGEC,GAAG,EAAE,kBAHP;MAIEH,QAAQ,EAAE;IAJZ,CAbW,CAAb;EAoBF;;AAlDa,CAAf","names":["AppHeader","AppTasks","AddTask","name","components","data","tasks","methods","addTask","task","deleteTask","id","confirm","filter","toggleReminder","map","reminder","created","text","day"],"sourceRoot":"","sources":["/home/user/Desktop/mirInternship/vue-vezba-2022/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <AppHeader title=\"TO DO LIST\" />\n    <AddTask @add-task=\"addTask\" />\n    <AppTasks @toggle-reminder=\"toggleReminder\" @delete-task=\"deleteTask\" :tasks= \"tasks\" />\n  </div>\n</template>\n\n<script>\nimport AppHeader from './components/AppHeader.vue'\nimport AppTasks from './components/AppTasks.vue'\nimport AddTask from './components/AddTask.vue'\n\nexport default {\n  name: 'App',\n  components: {\n    AppHeader,\n    AppTasks,\n    AddTask\n  },\n\n  data() {\n    return {\n      tasks: []\n    }\n  },\n\n  methods: {\n    addTask(task) {\n      this.tasks = [...this.tasks, task]\n    },\n    deleteTask(id) {\n      if(confirm('Are you sure?')) {\n        this.tasks = this.tasks.filter((task) => task.id !== id)\n      }\n    },\n\n    toggleReminder(id) {\n      this.tasks = this.tasks.map((task) => task.id === id ? {...task, reminder: !task.reminder} : task)\n    }\n  },\n\n  created() {\n    this.tasks = [\n      {\n        id: 1,\n        text: 'List 1',\n        day: 'Some Description',\n        reminder: true,\n      },\n      {\n        id: 2,\n        text: 'List 2',\n        day: 'Some Description',\n        reminder: true,\n      },\n      {\n        id: 3,\n        text: 'List 3',\n        day: 'Some Description',\n        reminder: false,\n      },\n    ]\n  }\n}\n</script>\n\n<style>\n@import url('https://fonts.googleapis.com/css2?family=Poppins:wght@300;400&display=swap');\n* {\n  box-sizing: border-box;\n  margin: 0;\n  padding: 0;\n}\nbody {\n  font-family: 'Poppins', sans-serif;\n}\n.container {\n  max-width: 500px;\n  margin: 30px auto;\n  overflow: auto;\n  min-height: 300px;\n  border: 1px solid steelblue;\n  padding: 30px;\n  border-radius: 5px;\n}\n.btn {\n  display: inline-block;\n  background: #000;\n  color: #fff;\n  border: none;\n  padding: 10px 20px;\n  margin: 5px;\n  border-radius: 5px;\n  cursor: pointer;\n  text-decoration: none;\n  font-size: 15px;\n  font-family: inherit;\n}\n.btn:focus {\n  outline: none;\n}\n.btn:active {\n  transform: scale(0.98);\n}\n.btn-block {\n  display: block;\n  width: 100%;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}