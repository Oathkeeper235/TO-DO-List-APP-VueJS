{"ast":null,"code":"import AppHeader from './components/AppHeader.vue';\nimport AppLists from './components/AppLists.vue';\nimport AddList from './components/AddList.vue';\nexport default {\n  name: 'App',\n  components: {\n    AppHeader,\n    AppLists,\n    AddList\n  },\n\n  data() {\n    return {\n      lists: [],\n      showAddList: false,\n      disabled: false\n    };\n  },\n\n  methods: {\n    beforeDestroy() {\n      if (this.lists.length > 9) {\n        this.$el.parentNode.removeChild(this.$el);\n      }\n    },\n\n    toggleAddList() {\n      this.showAddList = !this.showAddList;\n    },\n\n    addList(list) {\n      this.lists = [...this.lists, list];\n\n      if (this.lists.length > 9) {\n        this.toggleAddList();\n      }\n    },\n\n    deleteList(id) {\n      if (confirm('Are you sure?')) {\n        this.lists = this.lists.filter(list => list.id !== id);\n      }\n    },\n\n    toggleReminder(id) {\n      this.lists = this.lists.map(list => list.id === id ? { ...list,\n        reminder: !list.reminder\n      } : list);\n    }\n\n  },\n\n  created() {\n    this.lists = [{\n      id: 1,\n      text: 'List 1',\n      color: 'red',\n      reminder: true\n    }, {\n      id: 2,\n      text: 'List 2',\n      color: 'green',\n      reminder: true\n    }, {\n      id: 3,\n      text: 'List 3',\n      color: 'blue',\n      reminder: false\n    }];\n  }\n\n};","map":{"version":3,"mappings":"AAWA,OAAOA,SAAP,MAAsB,4BAAtB;AACA,OAAOC,QAAP,MAAqB,2BAArB;AACA,OAAOC,OAAP,MAAoB,0BAApB;AAEA,eAAe;EACbC,IAAI,EAAE,KADO;EAEbC,UAAU,EAAE;IACVJ,SADU;IAEVC,QAFU;IAGVC;EAHU,CAFC;;EAQbG,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,EADF;MAELC,WAAW,EAAE,KAFR;MAGLC,QAAQ,EAAE;IAHL,CAAP;EAKD,CAdY;;EAgBbC,OAAO,EAAE;IACPC,aAAY,GAAK;MACf,IAAG,KAAKJ,KAAL,CAAWK,MAAX,GAAoB,CAAvB,EAA0B;QACxB,KAAKC,GAAL,CAASC,UAAT,CAAoBC,WAApB,CAAgC,KAAKF,GAArC;MACF;IACD,CALM;;IAOPG,aAAa,GAAG;MACd,KAAKR,WAAL,GAAmB,CAAC,KAAKA,WAAzB;IACD,CATM;;IAWPS,OAAO,CAACC,IAAD,EAAO;MACZ,KAAKX,KAAL,GAAa,CAAC,GAAG,KAAKA,KAAT,EAAgBW,